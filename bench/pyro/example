type Latent
type Observation

proc Scale() -> real | lat : Latent | obs : Observation = {
  weight <- observe{lat}(NORMAL(8.5; 1.0));
  y <- sample{obs}(NORMAL(weight; 0.75));
  return weight
}

proc Guide(a: real; b: preal) -> unit | . | lat : Latent = {
  weight <- sample{lat}(NORMAL(a; b));
  return ()
}
