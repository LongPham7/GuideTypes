type Latent
type Observation

proc Model() -> real | lat : Latent | obs : Observation = {
  weight <- observe{lat}(D.normal(8.5; 1));
  y <- sample{obs}(D.normal(weight; 0.75));
  return weight
}

proc Guide[a: real; b: preal]() -> unit | . | lat : Latent = {
  weight <- sample{lat}(D.normal(a; b));
  return ()
}
