type Old_trace
type General
type General_no_old
type Init_type = &{ $ | real_u /\ Init_type[real_u /\ Init_type[real_u /\ Init_type[$]]] }

proc Recur() -> unit | old : Old_trace | lat : General = {
  if{lat} . then
    if{old} . then
      return ()
    else
      return ()
  else
    if{old}. then
      _ <- observe{old}(NORMAL(0; 1));
      _ <- sample{lat}(NORMAL(0; 1));
      _ <- Recur();
      _ <- observe{old}(NORMAL(0; 1));
      _ <- sample{lat}(NORMAL(0; 1));
      _ <- Recur();
      _ <- observe{old}(NORMAL(0; 1));
      _ <- sample{lat}(NORMAL(0; 1));
      Recur()
    else
      _ <- sample{lat}(NORMAL(0; 1));
      _ <- Recur();
      _ <- sample{lat}(NORMAL(0; 1));
      _ <- Recur();
      _ <- sample{lat}(NORMAL(0; 1));
      Recur()
}

proc Recur_no_old() -> unit | . | lat : General_no_old = {
  if{lat} . then
    return ()
  else
    _ <- sample{lat}(NORMAL(0; 1));
    _ <- Recur_no_old();
    _ <- sample{lat}(NORMAL(0; 1));
    _ <- Recur_no_old();
    _ <- sample{lat}(NORMAL(0; 1));
    Recur_no_old()
}

Initial_type: Init_type
Guide_composition: Recur
