type Latent
type Previous_trace_type = preal /\ +{ $ | ureal /\ $ }

proc Guide[a: preal; b: preal; c: preal; d: preal]() -> unit | old: Previous_trace_type | lat : Latent = {
  _ <- observe{old}(GAMMA(2; 1));
  v <- sample{lat}(GAMMA(a; b));
  if{lat} . then
    if{old} . then
      return ()
    else
      return()
  else
    if{old} . then
      _ <- observe{old}(BETA(c; d));
      _ <- sample{lat}(if (1 < 2) then SAME(ureal) else BETA(c; d));
      return()
    else
      _ <- sample{lat}(BETA(c; d));
      return()
}
